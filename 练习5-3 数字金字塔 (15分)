#include <stdio.h>

void pyramid( int n );

int main()
{    
    int n;

    scanf("%d", &n);
    pyramid(n);

    return 0;
}

/* 你的代码将被嵌在这里 */
void pyramid( int n )
{

    for(int i=1;i<n+1;i++)
    {
		
		for(int k=0;k<n-i;k++)
		{
			printf(" ");
		}
        for(int j=1;j<=i;j++)
        {
            printf("%d ",i);        
        }
        printf("\n");
    }
}
